@Article{Mangoldt*94,
  Title                    = {{Extract from a paper entitled: Zu Riemann's Abhandlung ``Ueber die Anzahl der Primzahlen unter einer gegebenen Gr\"osse''. (On Riemann's study ``On the number of primes less than a given bound''.). (Auszug aus einer Arbeit unter dem Titel: Zu Riemann's Abhandlung ``Ueber die Anzahl der Primzahlen unter einer gegebenen Gr\"osse''.)}},
  Author                   = {H. von Mangoldt},
  Journal                  = {Berl. Ber. 1894.},
  Year                     = {1894},
  Pages                    = {883-896},

  Abstract                 = {{Die in der Theorie der Riemann'schen Function $\xi(t)(\zeta(s))$ durch Hrn. Hadamard gemachten Fortschritte erm\"oglichen es dem Verf., zwei bisher unbewiesene Behauptungen Riemann's \"uber $\zeta(s)$ sicher zu stellen. Es seien (1) $\alpha_1$, $\alpha_2$, $\alpha_3$, ... diejenigen Wurzeln von $\xi(t)=0$, deren reelle Teile positiv sind, nach der Gr\"osse dieser Teile geordnet und jede so oft aufgenommen, als ihre Ordnungszahl angiebt, so ist nach Hadamard $$\xi(t) = \xi(0)\prod_{\nu=1}^\infty\left(1-\frac{t^2}{\alpha_\nu^2}\right).$$ Die Untersuchung des Zuwachses der Amplitude von $\xi(t)$ w\"ahrend einer geradlinigen Ver\"anderung des $t$ von $a-\frac32i$ nach $b-\frac32i$, wobei $a$, $b$ reell vorausgesetzt sind, f\"uhrt unter Benutzung Stieltjes'scher Ergebnisse \"uber die $\Gamma$ - Function zu den S\"atzen: $\xi(t)=0$ hat keine Wurzel, deren reeller Teil, absolut genommen, $\leqq12$ w\"are. Ist $h$ eine reelle Gr\"osse $>12$, so erweist sich die Anzahl der Gr\"ossen aus (1), deren reelle Teile $\leqq h$ sind, $$= \frac h{2\pi}l\frac h{2\pi} - \frac h{2\pi} + \frac54 + \eta\{0,34(lh)^2 + 1,34(lh) + 1,33\},$$ wobei $\eta$ zwischen $-1$ und 1 liegt. $\xi(t)=0$ hat wenigstens eine Wurzel mit einem reellen Teile $>0$ und $<53$.
F\"ur eine positive ganze Zahl $n$ werde $L(n)=0$ gesetzt, wenn $n=1$ ist oder verschiedene Primzahlen enth\"alt, $=l(p)$, wenn $n$ Potenz einer einzigen Primzahl $p$ ist. Es sei $r$ eine beliebige reelle Gr\"osse, $x$ reell und $>1$, und $$\Lambda(x,r) = \sum_{n=1}^{[x]} \frac{L(n)}{n^r}\quad\text{bez. }= \sum_{n=1}^x \frac{L(n)}{n^r} - \frac12\frac{L(x)}{x^r},$$ ersteres, wenn $x$ nicht Potenz einer Primzahl ist, letzteres, wenn solches statthat; endlich werde im ersteren Falle $$f(x,r) = \sum_{n=1}^{[x]}\frac{L(n)}{l(n)}\frac1{n^r} = \sum_1^x \frac1{p^r} + \frac12\sum_1^{x^\frac12} \frac1{p^{2r}} + \frac13\sum_1^{x^\frac13} \frac1{p^{3r}} +\cdots$$ gesetzt ($p$ hat jedesmal alle die Primzahlen zu durchlaufen, die nicht ausserhalb der Grenzen der Summation liegen), und dann im anderen Falle $f(x,r)=\frac12[f(x+0,r) + f(x-0,r)]$. Dabei wird nun $$f(x,r) = \int_r^\infty \Lambda(x,\varrho)\,d\varrho.$$ Den Gedanken, diese Beziehung bei der Herleitung des analytischen Ausdrucks von $f(x,r)$ zu Grunde zu legen, fand Hr. v. Mangoldt nachtr\"aglich auch in der Habilitationsschrift von Hrn. Piltz (Jena 1884) vor. --- Es sei $\alpha$ reell und $>0$ wie $>1-r$, so erh\"alt man auf Grund einiger einfacher bestimmter Integrale: $$-\lim_{h=\infty}\frac1{2\pi i} \int_{\alpha-hi}^{\alpha+hi} \frac{\zeta'(s+r)}{\zeta(s+r)} \frac{x^s}s ds = \Lambda(x,r),$$ wobei der Integrationsweg geradlinig zu denken ist; durch Zerlegung von $\frac{\zeta'(s+r)}{\zeta(s+r)}$ in Partialbr\"uche ergiebt sich dann: Setzt man $$\frac{2(r-\frac12)}{(r-\frac12)^2+\alpha_\nu^2} - 2x^{-r+\frac12}\frac{(r-\frac12)\cos(\alpha_\nu lx) - \alpha_\nu\sin(\alpha_\nu lx)}{(r-\frac12)^2 + \alpha_\nu^2} = W_\nu(x,r),$$ so convergirt $W_1(x,r)+W_2(x,r)+\cdots$ bei dieser Anordnung der Glieder f\"ur jedes reelle $r$ und jedes reelle $x>0$, und wenn $x>1$ ist, hat diese Reihe als Summe $$-\Lambda(x,r) + \frac{1-x^{1-r}}{r-1} - \frac12l\pi - \frac12C + \sum_{n=1}^\infty \frac{r+2nx^{-r-2n}}{2n(r+2n)};$$ $C$ bedeutet hier die Euler'sche Constante; wenn $r$ eine negative gerade Zahl ist, hat man f\"ur das Glied von unbestimmter Form den entsprechenden Grenzwert zu nehmen. Die ungleichm\"assig convergirende Reihe $\sum W_\nu(x,r)$ ist die Summe aus $$2x^{-r+\frac12}\sum_{\nu=1}^\infty \frac{\sin(\alpha_\nu lx)}{\alpha_\nu}$$ und aus einem Teile, der f\"ur alle positiven $x$ unbedingt und gleichmassig convergirt; f\"ur $r=\frac12$ verschwindet dieser Teil. Durch Integration der f\"ur $\Lambda(x,r)$ erhaltenen Formel in Bezug auf $r$ gewinnt man nun einen Ausdruck f\"ur $f(x,r)$. Es sind die F\"alle $r>1$, $-2<r\leqq1$, $r\leqq-2$ zu unterscheiden; in der dem zweiten Falle entsprechenden Entwickelung ist dann f\"ur $r=0$ genau diejenige Gleichung enthalten, welche Riemann f\"ur $f(x,0)$ durch Anwendung des Fourier'schen Satzes erzielt hat. --- Aus den Resultaten der Arbeit w\"urde sich eine Reihe interessanter asymptotischer Gesetze der Zahlenlehre ergehen, wenn es gelingen sollte, nachzuweisen, dass die Wurzeln von $\xi(t)=0$ ihre imagin\"aren Teile zwischen engeren Grenzen als $-\frac12i$ und $\frac12i$ haben, oder gar s\"amtlich reell sind.}},
  Classmath                = {{11M06 ($\zeta (s)$ and $L(s, \chi)$) 11M26 (Nonreal zeros of $\zeta (s)$ and $L(s, \chi)$; Riemann and other hypotheses) 11N05 (Distribution of primes) }},
  Keywords                 = {{Riemann's zeta function; $\xi$-function; $\Lambda$-function; Riemann hypothesis; distribution of primes}},
  Language                 = {German},
  Reviewer                 = {{Minkowski, Prof. (Z\"urich)}}
}

@Article{Mangoldt*97,
  Title                    = {{Proof of the equation $\sum\limits_{k=1}^\infty\frac{\mu(k)}k =0$. (Beweis der Gleichung $\sum\limits_{k=1}^\infty\frac{\mu(k)}k =0$.)}},
  Author                   = {Mangoldt, H. von},
  Year                     = {1897},

  Abstract                 = {{F\"ur ganze Zahlen $k&gt;0$ soll die Function $\mu(k)$ durch folgende Festsetzungen definirt sein: $\mu(1)=1$, $\mu(k)=(-1)^n$, falls $k$ das Product von $n$ verschiedenen Primzahlen $&gt;1$ ist, sonst $\mu(k)=0$. Euler behauptet (ohne strengen Beweis), dass die unendliche Reihe $\frac{\mu(1)}1+\frac{\mu(2)}2+\frac{\mu(3)}3+\cdots$ den Summenwert 0 habe. Der Verf. f\"uhrt den Beweis dieser Behauptung auf Grund seiner eigenen, sowie von Hadamard's und de la Vall\'ee-Poussin's Untersuchungen \"uber die Riemann'sche Function $\zeta(s)$. Die wesentliche Grundlage des Beweises ist die Formel: $$\lim_{n=\infty}\left\{\frac1{\log n} \sum_{k=1}^n\frac{\log k}k \sum_{\lambda=1}^k\frac{\mu(\lambda)}{\lambda}\right\}=0.\tag1$$ Der Verf. gelangt zu dieser Formel durch eine umst\"andliche Rechnung, welche sich einmal auf den beiden f\"ur jedes endliche $n$ g\"ultigen ,,H\"ulfss\"atzen'' $$-1\leqq\sum_{k=1}^n\frac{\mu(k)}k\leqq+1,\tag2$$ $$-(3+C)\leqq\log n\sum_{k=1}^n\frac{\mu(k)}k - \sum_{k=1}^n\frac{\mu(k)\log k}k\leqq3+C,\tag3$$ unter $C$ die Euler'sche Constante verstanden, aufbaut, andererseits aber einen wesentlichen Gebrauch von der Function $\Lambda(x,r)$ und ihrer analytischen Darstellung macht, welche der Verf. bei seinen genannten fr\"uheren Untersuchungen benutzt (cf. F. d. M. 25, 263, 1894, JFM 25.0263.01). Mit $u$ und $U$ bezeichnet der Verf. die untere, bez. obere Unbestimmtheitsgrenze der Summe $\sum\limits_{\lambda=1}^k\frac{\mu(\lambda)}{\lambda}$ f\"ur unbegrenzt wachsendes $k$. Auf Grund der Gleichung (1) zeigt sich ohne besondere M\"uhe, dass unm\"oglich $u&gt;0$ und dass gleichfalls nicht $U&lt;0$ sein kann; umst\"andlicher ist dagegen der Beweis der Unm\"oglichkeit von $U&gt;0$ und $u&lt;0$. Durch Zusammenfassung ergiebt sich die zu beweisende Gleichung $\sum\limits_{k=1}^\infty\frac{\mu(k)}k=0$. Weitere Folgerungen sind, dass die Summe $\sum\limits_{k=1}^n\mu(k)$ bei unbegrenzt wachsendem $n$ im Verh\"altnis zu $n$ verschwindend klein wird, sowie dass f\"ur sehr grosse $n$ zwischen 1 und $n$ ann\"ahernd ebenso viele ganze Zahlen vorkommen, die aus einer geraden, als solche, die aus einer ungeraden Anzahl von Primfactoren zusammengesetzt sind.}},
  Classmath                = {{*11A25 (Arithmetic functions, etc.) 11N25 (Distribution of integers with specified multiplicative constraints) }},
  Keywords                 = {{Mobius function}},
  Language                 = {German},
  Reviewer                 = {{Fricke, Prof. (Braunschweig)}}
}

