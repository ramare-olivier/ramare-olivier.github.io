@Article{Chowla-Pillai*30,
  Title                    = {{On the error terms in some asymptotic formulae in the theory of numbers. I.}},
  Author                   = {S.S. Pillai and S.D. Chowla},
  Journal                  = {Journal L. M. S.},
  Year                     = {1930},
  Pages                    = {95-101},
  Volume                   = {5},

  Abstract                 = {{$\varphi (n)$ sei die Anzahl der zu $n$ teilerfremden ganzen Zahlen $< n$. Dann ist bekanntlich $$ \sum\limits_1^x \varphi (n) = \dfrac{3}{\pi^2}\cdot x^2+E(x), $$ wo $E(x) = O(x\log x)$ ist. Die Verf. besch\"aftigen sich mit der genaueren Absch\"atzung dieses Restgliedes $E(x)$. Erstens wird bewiesen: $$ \sum\limits_1^R E(x)\sim\dfrac{3}{2\pi^2}R^2. $$ Zum Beweise wird der folgende dem {\it Axer}schen Satze \"ahnliche Hilfssatz benutzt: Wenn $\sum\limits_{n=1}^xa_n = o(x)$ und $|a_n|<K<\infty$ ist, so ist auch $$ \sum\limits_{n=1}^xa_n \left\{f\left(\dfrac{x}{n}\right)\right\}^2 = o(x), $$ wobei $f(x) = x - [x]$ bedeutet. Zweitens wird bewiesen: $$ E(x)\neq o(x \log \log \log x). $$ Hierzu wird zu jedem ganzzahligen $k$ ein $x_0$ konstruiert, so da{\ss} $$ k=O(\log\log x_0) $$ und $$ E(x_0+2k) - E(x_0) > H_3x_0\log\log\log x_0+O(x_0) $$ ist, woraus sich Satz 2 durch indirekten Beweis ergibt.}},
  Doi                      = {10.1112/jlms/s1-5.2.95},
  Language                 = {English},
  Reviewer                 = {{Rothe-Ille, Hildegard; Dr. (Breslau)}}
}

