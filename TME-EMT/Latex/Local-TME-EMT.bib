@Article{Petermann*98,
  Title                    = {{On an estimate of Walfisz and Saltykov for an error term related to the Euler function.}},
  Author                   = {Y.-F.S. P\'etermann},
  Journal                  = {J. Th\'eor. Nombres Bordx.},
  Year                     = {1998},
  Number                   = {1},
  Pages                    = {203-236},
  Volume                   = {10},

  Abstract                 = {{The author uses the techniques developed by A. Walfisz for the estimation of the error term $$ \sum_{n \leq x} {\varphi(n) \over n} - {6 \over \pi^2} \cdot x = O((\log x)^{2 \over 3} \cdot (\log \log x)^{4 \over 3}), $$ in a simplified version, which comes from exploiting papers of {\it A. A. Karatsuba} [Tr. Mat. Inst. Steklov 112, 241-255 (1971; Zbl 0226.10037)] and {\it A. I. Saltykov} [Vestn. Mosk. Univ., Ser. I 15, No. 6, 34-50 (1961; Zbl 0099.03702)]. Thus, the author is able to give [rather] sharp estimates for the remainder terms in the asymptotic formulae for $$ \sum_{n \leq x} \left( {\sigma(n) \over n}\right)^r,\quad \sum_{n\leq x}\left({\varphi(n)\over n}\right)^r,\quad \sum_{n \leq x} \left( {\sigma(n) \over \varphi(n)}\right)^r. $$ These results are deduced from the following main results. \par (1) For any $ \Lambda > 0 $ there is a $ {\cal B} $ such that $$ \sum_{p \leq N^{1 \over k}} \exp\left(2\pi i {x \over p^k}\right) \ll N^{1 \over k} \cdot {1 \over (\log N)^\Lambda} $$ for large $N$ and $x$ satisfying $\exp ({\cal B} t^{2 \over 3} u^{4 \over 3}) \leq x \cdot \exp(-t^b)$, where $0 <b <1$, $t = \log x$, $u= \log \log x $. \par (2) Under suitable conditions on the multiplicative arithmetical function $ n \mapsto v(n)$, the estimate $$ \sum_{n \leq y} v(n) \cdot \psi \left( {x \over n} \right) =O\left(t^{2(\alpha+1) \over 3} \cdot u^{4(\alpha+1) \over 3}\right) $$ holds. Here $ \psi(x) = x - [x] - {1 \over 2}. $ \par Finally, the author points out that, for the Euler-function $ \varphi $, Saltykov's method does not give a sharper result than Walfisz's method.}},
  Classmath                = {{11N37 (Asymptotic results on arithmetic functions) 11A25 (Arithmetic functions, etc.) }},
  Doi                      = {10.5802/jtnb.225},
  File                     = {:/home/ramare/tex/Other/Petermann/JTNB_1998__10_1_203_0.pdf:PDF},
  Keywords                 = {{asymptotic formulae for the Euler function with remainder term; sum of divisors; estimation of exponential sums with primes; multiplicative arithmetical functions; Walfisz method}},
  Language                 = {English},
  Reviewer                 = {{Wolfgang Schwarz (Frankfurt am Main)}}
}

