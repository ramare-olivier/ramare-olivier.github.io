@Article{Axer*11,
  Title                    = {{\"U}ber einige {G}renzwerts{\"a}tze},
  Author                   = {A. Axer},
  Journal                  = {Wien. Ber.},
  Year                     = {1911},
  Pages                    = {1253-1298},
  Volume                   = {120},
  Abstract                 = {{Der Verf. verallgemeinert einen fr\"uher von ihm gefundenen Satz (siehe F. d. M. 41, 240, 1910) auf gewisse Funktionen zweier Variablen. Als Anwendung folgt der Beweis, da{\ss} die Formel $$\sum_{n=1}^x\frac{\mu(n)}n\text{lg}\frac xn=1+O(1)$$ elementar aus $$\sum_{n=1}^x\frac{\mu(n)}n=O(1)$$ folgt. Die Verallgemeinerung lautet: Die Funktion $f(z,n)$ sei f\"ur alle Wertepaare $(z,n)$ mit $z\geqq1$ und n\"aturlichem $n\leqq z$ definiert, und sie erf\"ulle, wenn $$\sum_{n=1}^x|f(z,n)|=E(z,x),\quad\sum_{n=1}^xf(z,n)=F(z,x)$$ gesetzt wird, die zwei Bedingungen $$\aligned \format \r\ \quad &amp;\r\ E(ux,x)=xO(u), &amp;F(ux,x)=O(x).\ (x&gt;0) &amp;(1\leqq u\leqq U) \endaligned$$ ($U\leqq1$ beliebig gro\ss, aber fest). Dan gilt f\"ur jedes $r&gt;0$; $$S_r(x)=\sum_{n=1}^x\left(\frac{x^r}{n^r}\left[\frac{x^r}{n^r}\right]\right) f(x,n)=O(x).$$}},
  Language                 = {German},
  Reviewer                 = {{Fueter, Prof. (Karlsruhe)}}
}

