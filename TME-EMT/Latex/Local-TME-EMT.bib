@Article{Landau*08-1,
  Title                    = {{On prime numbers in an arithmetic progression and prime ideals in a class of ideals. (\"Uber die Primzahlen in einer arithmetischen Progression und die Primideale in einer Idealklasse.)}},
  Author                   = {E. Landau},
  Journal                  = {Wien. Ber.},
  Year                     = {1908},
  Pages                    = {1095-1107},
  Volume                   = {117},

  Abstract                 = {{Es seien $k$ und $l$ teilerfremde Zahlen $>0$ und $\varrho (x)$ die Anzahl der Primzahlen $ky+l\leqq x$. Dann ist $$\varrho(x)=\frac{1}{\varphi(k)}\ \int_2^x\ \frac{du}{\text{lg\,}u}+O(xe-\root\gamma\of{\text{lg\,}x}),$$ wo $\gamma$ eine von $k$ abh\"angige Konstante ist. Dieses Resultat wird hier so erweitert, da{\ss} mittels elementarer H\"ulfsmittel die obige Formel bei ganz beliebigem konstantem $gamma>8$ bewiesen wird. Auf die Anzahl der Primideale in einer Klasse eines algebraischen K\"orpers der Klassenanzahl $h$ angewandt, lautet die entsprechende Formel: $$\varrho(x)=\frac{1}{h}\ \int_2^x\frac{du}{\text{lg\,}u}+O(xe-\root\gamma\of{\text{lg\,}x}).$$}},
  Classmath                = {{11N13 (Primes in progressions) 11R44 (Distribution of prime ideals) }},
  Keywords                 = {{primes in progressions; prime ideals in ideal classes}},
  Language                 = {German},
  Reviewer                 = {{Fueter, Prof. (Basel)}}
}

@Article{Landau*08a,
  Title                    = {{A new proof of Riemann's formula for the number of prime numbers smaller than a given limit, and a proof of a more general formula for the case of prime numbers of an arithmetic progression. (Nouvelle d\'emonstration pour la formule de {\it Riemann} sur le nombre des nombres premiers inf\'erieurs \`a une limite donn\'ee et d\'emonstration d'une formule plus g\'en\'erale pour le cas des nombres premiers d'une progression arithm\'etique.)}},
  Author                   = {E. Landau},
  Journal                  = {Ann. de \'Ec. Norm. (3)},
  Year                     = {1908},
  Pages                    = {399-448},
  Volume                   = {25},

  Abstract                 = {{In der Einleitung gibt der Verf. eine historische W\"urdigung der bisherigen Resultate der Theorie des Primzahlproblems. Es handelt sich um die Arbeiten von {\it Riemann, Hadamard} und {\it v. Mangoldt}. Im ersten Teil der Arbeit bringt er den schon oben besprochenen neuen und einfachen Beweis des {\it Riemann}schen Resultats. Im zweiten Teil wird vom Verf. das Problem der Anzahl der Primzahlen $<x$, die durch eine arithmetische Progression $ky+l$ ($k$ und $l$ ohne gemeinsamen Teiler) dargestellt werden, behandelt. Er gibt eine Formel an, die im Falle $k=1$ in die {\it Riemann}sche \"ubergeht.}},
  Classmath                = {{11N05 (Distribution of primes) 11N13 (Primes in progressions) }},
  Keywords                 = {{prime number theorem; primes in progressions}},
  Language                 = {French},
  Reviewer                 = {{Fueter, Prof. (Basel)}}
}

